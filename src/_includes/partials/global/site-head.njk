<a class="skip-link" href="#main-content">Skip to content</a>
<header role="banner" class="[ site-head ] [ pad-top-500 pad-bottom-500 ">
  <div class="wrapper">
    <div class="[ site-head__inner ] [ md:box-flex space-between align-center ]">
      <a href="/" class="[ site-head__site-name ] [ leading-tight ]">
        <span class="visually-hidden">{{ site.name }} - Home</span>
        <span class="visually-hidden [ site-head__site-name ] [ text-600 ]" aria-hidden="true">{{ site.name }}</span>
        <div class="site-head__logo">
          <img class="site-head__logo-img"src="../../images/proud-ounces.png" alt="Proud Ounces" title="Proud Ounces" />
        </div>
      </a>
      {% set ariaLabel = 'navigation' %}
      {% include "partials/components/nav.njk" %}
    </div>
  </div>
</header>
