{% extends 'layouts/base.njk' %}

{# Intro content #}
{% set introHeading = title %}
{% set introClass = "ta-center" %}
{% set introSummary %}
  {{ content | safe }}
{% endset %}

{# Marquee content #}
{% if tickerHed %}
  {% set ticker %}
    {{ tickerHed | safe }}
  {% endset %}
{% endif %}

{# Callout content #}
{% set introCallout %}{{ callout | safe }}{% endset %}

{% block content %}
  <main id="main-content" tabindex="-1">
    {% include "partials/components/intro.njk" %}
    {% include "partials/animations/topwave.njk" %}
    {% include "partials/components/po-services.njk" %}
    {% include "partials/components/homepage-form.njk" %}

    {# {% include "partials/components/pumpcoaching-callout.njk" %} #}
    {# {% include "partials/components/callout.njk" %} #}
  </main>
{% endblock %}
